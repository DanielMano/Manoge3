<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.daniel.manoge3">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WeightGraph" />
        <activity android:name=".InputActivity" />
        <activity
            android:name=".DatabaseActivity"
            android:label="@string/title_activity_database"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".InputItemActivity"
            android:label="@string/title_activity_input_item"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".BeginWorkout"
            android:label="@string/title_activity_begin_workout"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity android:name=".RoutineExercisesActivity"
            android:label="RoutineExercisesActivity"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>